@page "/not-found"
@layout ErrorLayout
@inject NavigationManager Navigation
@inject IStringLocalizer<_404> L

<MudText Typo="Typo.h3" Color="Color.Primary"><b>404</b> - @L["PageNotFound"]</MudText>

<MudButton Color="Color.Primary" Variant="Variant.Filled" OnClick="@(() => Navigation.NavigateTo("/home"))">@L["ToHome"]</MudButton>
